<div class="container">
	<h2>Lyceum Theatre</h2>
	{{#each seats}}
		<div class="panel panel-default">
			<div class="panel-heading">
				<h3 class="panel-title">{{name}}</h3>
			</div>
			<ul class="list-group">
				{{#each this.bands}}
					{{> _seatStub}}
				{{/each}}
			</ul>
		</div>
	{{/each}}
</div>